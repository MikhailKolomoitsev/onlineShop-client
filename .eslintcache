fo = () => {\r\n        setInfo([...info, { title: '', description: '', number: Date.now() }])\r\n    }\r\n    const deleteInfo = (number) => {\r\n        setInfo(info.filter(i => i.number !== number))\r\n    }\r\n    const selectFile = e => {\r\n        setfile(e.target.files[0])\r\n    }\r\n    const changeInfo = (key, value, number) => {\r\n        setInfo(info.map(i => i.number === number ? { ...i, [key]: value } : i))\r\n    }\r\n    const addDevice = () => {\r\n        const formData = new FormData() //Form data because we upload file\r\n        formData.append('name', name)\r\n        formData.append('price', `${price}`)\r\n        formData.append('img', file)\r\n        formData.append('brandId', device.selectedBrand.id)\r\n        formData.append('typeId', device.selectedType.id)\r\n        formData.append('info', JSON.stringify(info))\r\n        createDevice(formData).then(data => onHide())\r\n    }\r\n\r\n    useEffect(() => {\r\n        fetchTypes().then(data => device.setTypes(data))\r\n        fetchBrands().then(data => device.setBrands(data))\r\n    }, [])\r\n\r\n    return (\r\n        <Modal\r\n            show={show}\r\n            onHide={onHide}\r\n            size=\"lg\"\r\n            centered\r\n        >\r\n            <Modal.Header closeButton>\r\n                <Modal.Title id=\"contained-modal-title-vcenter\">\r\n                    Add the New Device\r\n                </Modal.Title>\r\n            </Modal.Header>\r\n            <Modal.Body>\r\n                <Form className=''>\r\n                    <Dropdown className='mt-3'>\r\n                        <Dropdown.Toggle\r\n                            className='mr-2'>\r\n                            {device.selectedType.name || \"Choose type\"}\r\n                        </Dropdown.Toggle>\r\n                        <Dropdown.Menu>\r\n                            {device._types.map(type =>\r\n                                <Dropdown.Item\r\n                                    onClick={() => device.setSelectedType(type)}\r\n                                    key={type.id}>\r\n                                    {type.name}\r\n                                </Dropdown.Item>)}\r\n                        </Dropdown.Menu>\r\n                    </Dropdown>\r\n                    <Dropdown className='mt-3'>\r\n                        <Dropdown.Toggle\r\n                            className='mr-2'>{device.selectedBrand.name || \"Choose brand\"}\r\n                        </Dropdown.Toggle>\r\n                        <Dropdown.Menu>\r\n                            {device._brands.map(brand =>\r\n                                <Dropdown.Item\r\n                                    onClick={() => device.setSelectedBrand(brand)}\r\n                                    key={brand.id}>\r\n                                    {brand.name}\r\n                                </Dropdown.Item>)}\r\n                        </Dropdown.Menu>\r\n                    </Dropdown>\r\n                    <Form.Control\r\n                        value={name}\r\n                        onChange={e => setname(e.target.value)}\r\n                        className='mt-3'\r\n                        placeholder='Insert name'\r\n                    />\r\n                    <Form.Control\r\n                        value={price}\r\n                        onChange={e => setprice(Number(e.target.value))}\r\n                        className='mt-3'\r\n                        placeholder='Insert price'\r\n                        type='number'\r\n                    />\r\n                    <Form.Control\r\n                        className='mt-3'\r\n                        placeholder='Upload picture'\r\n                        type='file'\r\n                        onChange={selectFile}\r\n                    />\r\n                    <hr></hr>\r\n                    <Button\r\n                        variant={'outline-dark'}\r\n                        onClick={addInfo}\r\n                    >\r\n                        Add new property\r\n                    </Button>\r\n                    {info.map(i =>\r\n                        <Row className='mt-3' key={i.number}>\r\n                            <Col md={4}>\r\n                                <Form.Control\r\n                                    value={i.title}\r\n                                    onChange={(e)=>changeInfo(\"title\", e.target.value, i.number)}\r\n                                    placeholder='Name of the property'\r\n                                />\r\n                            </Col>\r\n                            <Col md={4}>\r\n                                <Form.Control\r\n                                    value={i.description}\r\n                                    onChange={(e)=>changeInfo(\"description\", e.target.value, i.number)}\r\n                                    placeholder='Desc of the property'\r\n                                />\r\n                            </Col>\r\n                            <Col md={4}>\r\n                                <Button\r\n                                    variant={'outline-danger'}\r\n                                    onClick={() => deleteInfo(i.number)}\r\n                                >\r\n                                    Delete\r\n                                </Button>\r\n                            </Col>\r\n                        </Row>)}\r\n                </Form>\r\n            </Modal.Body>\r\n            <Modal.Footer>\r\n                <Button variant='outline-danger' onClick={onHide}>Close</Button>\r\n                <Button variant='outline-success' onClick={addDevice}>Add</Button>\r\n            </Modal.Footer>\r\n        </Modal>\r\n    )\r\n})\r\n\r\nexport default CreateDevice\r\n","C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\onlineShop-client\\src\\components\\modals\\CreateBrand.js",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\onlineShop-client\\src\\components\\DeviceItem.